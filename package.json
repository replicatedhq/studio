    "login-cli": "npm-cli-login",
    "publish-if-version-changed": "publish",
    "snyk-protect": "snyk protect"
  },
  "bin": "bin/replicated-studio",
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/node": "^8.0.57",
    "@types/tv4": "^1.2.28",
    "chai": "3.5.0",
    "mocha": "^3.2.0",
    "publish": "^0.6.0",
    "tslint": "^4.5.1",
    "typescript": "^2.2.1"
  },
  "dependencies": {
    "body-parser": "^1.17.0",
    "chalk": "^1.1.3",
    "cors": "^2.8.1",
    "express": "^4.15.0",
    "express-http-proxy": "^0.11.0",
    "js-yaml": "^3.8.2",
    "lodash": "^4.17.4",
    "npm-cli-login": "^0.0.10",
    "replicated-lint": "^0.18.3",
    "source-map-support": "^0.4.11",
